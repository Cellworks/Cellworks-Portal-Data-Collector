[
    {
        "storefront": "Mobile Sentrix",
        "devices": {
            "iphone": {
                "linkPath": "apple/iphone-parts",
                "version": {
                    "14-pro-max": "iphone-14-pro-max",
                    "14-pro": "iphone-14-pro",
                    "14-plus": "iphone-14-plus",
                    "14": "iphone-14",
                    "13-pro-max": "iphone-13-pro-max",
                    "13-pro": "iphone-13-pro",
                    "13": "iphone-13",
                    "13-mini": "iphone-13-mini",
                    "se-22": "iphone-se-2022",
                    "12-pro-max": "iphone-12-pro-max",
                    "12-pro": "iphone-12-pro",
                    "12": "iphone-12",
                    "12-mini": "iphone-12-mini",
                    "11-pro-max": "iphone-11-pro-max",
                    "11-pro": "iphone-11-pro",
                    "11": "iphone-11",
                    "xs-max": "iphone-xs-max",
                    "xs": "iphone-xs",
                    "xr": "iphone-xr",
                    "x": "iphone-x",
                    "se-20": "iphone-se-2020",
                    "8-plus": "iphone-8-plus",
                    "8": "iphone-8",
                    "7-plus": "iphone-7-plus",
                    "7": "iphone-7",
                    "6s-plus": "iphone-6s-plus",
                    "6s": "iphone-6s",
                    "6-plus": "iphone-6-plus",
                    "6": "iphone-6"
                }
            }
        },
        "link": "https://www.mobilesentrix.com/replacement-parts/<device>/<version>/?p=<page>",
        "pages": {
            "multiple": true,
            "itemAmount": "#catalog-listing > div.category-products.main-list-category.deal-category > div.toolbar > div.sorter > div.amount",
            "itemPerPage": 120,
            "removeFromItemAmount": " Item(s)"
        },
        "container": "#catalog-listing > div.category-products.main-list-category.deal-category > ul > li.item",
        "elements": {
            "link": {
                "selector": "fieldset > div.cat_prod_cell > a",
                "attribute": "href"
            },
            "name": {
                "selector": "fieldset > div.cat_prod_cell > a",
                "attribute": "title"
            },
            "cost": {
                "selector": "fieldset > div.cat_prod_cell > div.price-box > span > span.price"
            },
            "image": {
                "selector": "fieldset > div.cat_prod_cell > a > img.small-img",
                "attribute": "data-original"
            },
            "badge_image": {
                "selector": "fieldset > div.cat_prod_cell > a > div.product-budges > img.badgesImg",
                "attribute": "src"
            }
        }
    },
